from typing import List

class Solution:
    def climbStairs(self, n: int, costs: List[int]) -> int:
        dp = [0]*(n+1)
        
        for i in range(1, n+1):
            dp[i] = min(
                costs[i-1] + 1 + dp[i-1],
                costs[i-1] + 4 + dp[i-2] if i-2>= 0 else float('inf'),
                costs[i-1] + 9 + dp[i-3] if i-3>= 0 else float('inf'),
            )
        
        return dp[n]

if __name__=='__main__':
    s = Solution()

    s.climbStairs(4, [1, 2, 3, 4])
